<?xml version="1.0" encoding="UTF-8"?>
<M2:ArchitecturalSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:M2="http://www.example.org/M2"
    xsi:schemaLocation="http://www.example.org/M2 asa.ecore#//M2">
  <architecturalElements
      xsi:type="M2:Configuration"
      name="Client_Server"
      components="//@architecturalElements.0/@elements.0 //@architecturalElements.0/@elements.1"
      connectors="//@architecturalElements.0/@elements.2">
    <elements
        xsi:type="M2:Component"
        name="Client"
        configuration="//@architecturalElements.0">
      <icomponents
          xsi:type="M2:ProvidedPort"
          name="Send_Request"/>
    </elements>
    <elements
        xsi:type="M2:Component"
        name="Server"
        configuration="//@architecturalElements.0">
      <elements
          xsi:type="M2:Configuration"
          name="Server_Detail"
          components="//@architecturalElements.0/@elements.1/@elements.0/@elements.0">
        <elements
            xsi:type="M2:Component"
            name="ConnectionManager"
            configuration="//@architecturalElements.0/@elements.1/@elements.0">
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="External_Socket"/>
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="Security_Check"/>
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="DB_Query"/>
        </elements>
        <elements
            xsi:type="M2:Component"
            name="Security_Manager">
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="Security_Autentification"/>
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="Check_Query"/>
        </elements>
        <elements
            xsi:type="M2:Component"
            name="Database">
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="Security_Management"/>
          <icomponents
              xsi:type="M2:ProvidedPort"
              name="Query_Interrogation"/>
        </elements>
        <elements
            xsi:type="M2:Connector"
            name="CM_SM">
          <iconnectors
              xsi:type="M2:ProvidedRole"
              name="Caller_CM_SM"/>
          <iconnectors
              xsi:type="M2:RequiredRole"
              name="Called_CM_SM"/>
        </elements>
        <elements
            xsi:type="M2:Connector"
            name="SM_DB">
          <iconnectors
              xsi:type="M2:ProvidedRole"
              name="Caller_SM_DB"/>
          <iconnectors
              xsi:type="M2:RequiredRole"
              name="Called_SM_DB"/>
        </elements>
        <elements
            xsi:type="M2:Connector"
            name="DB_CM">
          <iconnectors
              xsi:type="M2:ProvidedRole"
              name="Caller_DB_CM"/>
          <iconnectors
              xsi:type="M2:RequiredRole"
              name="Called_DB_CM"/>
        </elements>
        <elements
            xsi:type="M2:Attachment"
            name="CM_To_CM_SM"
            ports="//@architecturalElements.0/@elements.1/@elements.0/@elements.0/@icomponents.1"
            roles="//@architecturalElements.0/@elements.1/@elements.0/@elements.3/@iconnectors.0"/>
        <elements
            xsi:type="M2:Attachment"
            name="CM_SM_To_SM"
            ports="//@architecturalElements.0/@elements.1/@elements.0/@elements.1/@icomponents.0"
            roles="//@architecturalElements.0/@elements.1/@elements.0/@elements.3/@iconnectors.1"/>
        <elements
            xsi:type="M2:Attachment"
            name="SM_To_SM_DB"
            ports="//@architecturalElements.0/@elements.1/@elements.0/@elements.1/@icomponents.1"
            roles="//@architecturalElements.0/@elements.1/@elements.0/@elements.4/@iconnectors.0"/>
        <elements
            xsi:type="M2:Attachment"
            name="SM_DB_To_DB"
            ports="//@architecturalElements.0/@elements.1/@elements.0/@elements.2/@icomponents.0"
            roles="//@architecturalElements.0/@elements.1/@elements.0/@elements.4/@iconnectors.1"/>
        <elements
            xsi:type="M2:Attachment"
            name="DB_To_DB_CM"
            ports="//@architecturalElements.0/@elements.1/@elements.0/@elements.2/@icomponents.1"
            roles="//@architecturalElements.0/@elements.1/@elements.0/@elements.5/@iconnectors.0"/>
        <elements
            xsi:type="M2:Attachment"
            name="DB_CM_To_CM"
            ports="//@architecturalElements.0/@elements.1/@elements.0/@elements.0/@icomponents.2"
            roles="//@architecturalElements.0/@elements.1/@elements.0/@elements.5/@iconnectors.1"/>
        <elements
            xsi:type="M2:Binding"
            name="ConnectionManager_Server"
            ports="//@architecturalElements.0/@elements.1/@icomponents.0 //@architecturalElements.0/@elements.1/@elements.0/@elements.0/@icomponents.0"/>
      </elements>
      <icomponents
          xsi:type="M2:RequiredPort"
          name="Receive_Request"/>
    </elements>
    <elements
        xsi:type="M2:Connector"
        name="RPC">
      <iconnectors
          xsi:type="M2:ProvidedRole"
          name="Caller"/>
      <iconnectors
          xsi:type="M2:RequiredRole"
          name="Called"/>
    </elements>
    <elements
        xsi:type="M2:Attachment"
        name="Client_To_RPC"
        ports="//@architecturalElements.0/@elements.0/@icomponents.0"
        roles="//@architecturalElements.0/@elements.2/@iconnectors.0"/>
    <elements
        xsi:type="M2:Attachment"
        name="RPC_To_Server"
        ports="//@architecturalElements.0/@elements.1/@icomponents.0"
        roles="//@architecturalElements.0/@elements.2/@iconnectors.1"/>
    <elements
        xsi:type="M2:Binding"
        name="Server_System"
        ports="//@architecturalElements.0/@ports.0 //@architecturalElements.0/@ports.1"/>
    <elements
        xsi:type="M2:Binding"
        name="Client_System"
        ports="//@architecturalElements.0/@elements.1/@icomponents.0 //@architecturalElements.0/@elements.0/@icomponents.0"/>
    <ports xsi:type="M2:RequiredPort"
        name="Server_port"/>
    <ports xsi:type="M2:ProvidedPort"
        name="Client_port"/>
  </architecturalElements>
</M2:ArchitecturalSystem>
